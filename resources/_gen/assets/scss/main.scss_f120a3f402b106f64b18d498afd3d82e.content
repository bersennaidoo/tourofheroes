body {
  font-size: 100%;
  font-family: Roboto, sans-serif;
  line-height: 1.5;
  margin: unset;
  background-color: #f2faff;
  color: black; }

.main-container {
  display: grid;
  max-inline-size: 25.25rem;
  grid-template-columns: 1fr;
  grid-template-rows: repeat(5, auto);
  grid-template-areas: "header header" "navbar navbar" "main main" "sidebar-top sidebar-top" "sidebar-bottom sidebar-bottom";
  gap: 1rem;
  margin-inline: auto;
  margin-block: 0; }

.c-headerp {
  grid-column: header; }
  .c-headerp__heading {
    font-weight: bold;
    font-size: 2.027rem;
    text-align: center;
    padding-block-start: 2rem; }

.c-navp {
  grid-column: navbar; }
  .c-navp__site-nav {
    display: flex;
    flex-direction: column;
    gap: 0.5rem;
    margin: 0;
    padding-inline: 0.5rem;
    padding-block: 0.5rem;
    list-style-type: none; }
  .c-navp__item {
    flex: 1 1 auto; }
  .c-navp__dropdown {
    display: none; }
  .c-navp__link {
    display: block;
    padding-inline: 1rem;
    padding-block: 0.5rem;
    background-color: #0072b0;
    color: white;
    font-size: 1.125rem;
    text-decoration: none; }
  .c-navp__toggle {
    padding-inline: 0.5rem;
    padding-block: 0.5rem;
    background-color: #0072b0;
    color: white; }

.c-main-home {
  grid-column: main; }
  .c-main-home--tile {
    padding-inline: 1.5rem;
    padding-block: 1.5rem;
    background-color: #fff; }
  .c-main-home--tile > :first-child {
    margin-block-start: 0; }

.c-sidebar-top {
  grid-column: sidebar-top; }
  .c-sidebar-top__sheading {
    margin: 0;
    font-size: 0.9rem;
    font-weight: bold;
    text-align: right;
    text-transform: uppercase; }
  .c-sidebar-top__iusername, .c-sidebar-top__ipassword {
    display: block;
    inline-size: 100%; }
  .c-sidebar-top__button {
    margin-block-start: 1rem;
    border: 0.063rem solid #0072b0;
    background-color: white;
    padding-inline: 1rem;
    padding-block: 0.5rem;
    cursor: pointer; }
  .c-sidebar-top--centered {
    text-align: center; }

.c-sidebar-bottom {
  grid-column: sidebar-bottom; }
  .c-sidebar-bottom__cost {
    display: flex;
    justify-content: center;
    align-items: center;
    line-height: 0.7; }
  .c-sidebar-bottom__cost-currency {
    font-size: 2rem; }
  .c-sidebar-bottom__cost-dollars {
    font-size: 4rem; }
  .c-sidebar-bottom__cost-cents {
    font-size: 1.5rem; }
  .c-sidebar-bottom__cta-button {
    display: block;
    background-color: #0072b0;
    color: white;
    padding-inline: 8.55rem;
    padding-block: 0.5rem;
    text-decoration: none; }

